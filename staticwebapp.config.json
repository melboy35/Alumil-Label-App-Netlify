{
  "routes": [
    {
      "route": "/admin.html",
      "allowedRoles": [
        "authenticated"
      ]
    },
    {
      "route": "/**",
      "allowedRoles": [
        "anonymous"
      ]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "globalHeaders": {
    "Cache-Control": "no-store"
  }
}